{
  "name": "takeaway-discord-bot",
  "module": "index.ts",
  "private": "true",
  "license": "GPL-3.0-only",
  "dependencies": {
    "discord.js": "^14.25.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "start:prod": "bun --env-file=.env.production run src/index.ts",
    "start:dev": "bun --env-file=.env.development run src/index.ts",
    "deploy:prod": "bun --env-file=.env.production run src/deploy-commands.ts",
    "deploy:dev": "bun --env-file=.env.development run src/deploy-commands.ts",
    "format": "prettier --write src/"
  },
  "type": "module"
}
